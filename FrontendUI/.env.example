# FrontendUI environment variables

# Backend API base URL, e.g. https://api.modelmapper.example.com or http://localhost:4000
REACT_APP_API_BASE_URL=

# Enable API mocks at runtime (intercepts fetch). Use "true" for local demo/tests only.
# When "true", src/index.js calls enableApiMocks() from src/mocks/browser.js.
REACT_APP_ENABLE_MOCKS=false

# Enable authentication enforcement in routes. When "true", ProtectedRoute requires a token
# and enforces optional role checks; when "false", routes are publicly accessible.
REACT_APP_ENABLE_AUTH=false
